extends layout
block content
	.container
		.row
			h2 List of spookmarks
			table(width="100%")
				each d, index in spookmarks
					tr
						td= d.uniqueID
						td= d.userID
						td
							-var link = decodeURIComponent(d.URL);
							a(href=link)= d.title
						td= d.note
						td= moment(d.futureDate).fromNow()